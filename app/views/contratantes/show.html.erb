<p id="notice"><%= notice %></p>

<%if @contratante.logo_url != nil %>
<p>
  <img src="<%= @contratante.logo_url%>" style="max-width:100px;max-height:100px">
</p>
<%end%>

<p>
  <strong>Nome:</strong>
  <%= @contratante.nome %>
</p>

<p>
  <strong>Email:</strong>
  <%= @contratante.email %>
</p>

<p>
  <strong>Telefone:</strong>
  <%= @contratante.telefone %>
</p>

<p>
  <strong>Logradouro:</strong>
  <%= @contratante.logradouro %>
</p>

<p>
  <strong>Complemento:</strong>
  <%= @contratante.complemento %>
</p>

<p>
  <strong>Numero:</strong>
  <%= @contratante.numero %>
</p>

<p>
  <strong>Bairro:</strong>
  <%= @contratante.bairro %>
</p>

<p>
  <strong>Cep:</strong>
  <%= @contratante.cep %>
</p>

<p>
  <strong>Municipio:</strong>
  <%= @contratante.municipio %>
</p>


<% if @contratante.cnpj != nil && @contratante.cnpj != ""%>
<p>
  <strong>Cnpj:</strong>
  <%= @contratante.cnpj %>
</p>

<p>
  <strong>Inscricaoestadual:</strong>
  <%= @contratante.inscricaoEstadual %>
</p>
<%end%>

<% if @contratante.cpf != nil && @contratante.cpf != ""%>
<p>
  <strong>CPF:</strong>
  <%= @contratante.cpf %>
</p>
<%end%>

<p>
  <strong>Autorizado:</strong>
  <%= current_user.status %>
</p>

<%= link_to 'Editar', edit_contratante_path(@contratante) %> |
<%= link_to 'Voltar', root_path %>
