<p id="notice"><%= notice %></p>

<h1>Avaliar candidaturas a vaga </h1>

<table>
  <thead>
    <tr>
      <th>Candidato</th>
      <th>Email</th>
      <th>Telefone</th>
      <th>Munic√≠pio</th>
      <th>Nota</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @candidaturas.each do |candidatura| %>
      <tr>
        <td><%= candidatura.candidato.nome %></td>
        <td><%= candidatura.candidato.email %></td>
        <td><%= candidatura.candidato.telefone %></td>
