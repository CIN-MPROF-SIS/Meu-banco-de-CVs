<p id="notice"><%= notice %></p>

<p>
  <strong>Nome:</strong>
  <%= @candidato.nome %>
</p>

<p>
  <strong>Email:</strong>
  <%= @candidato.email %>
</p>

<p>
  <strong>Telefone:</strong>
  <%= @candidato.telefone %>
</p>

<p>
  <strong>Logradouro:</strong>
  <%= @candidato.logradouro %>
</p>

<p>
  <strong>Complemento:</strong>
  <%= @candidato.complemento %>
</p>

<p>
  <strong>Numero:</strong>
  <%= @candidato.numero %>
</p>

<p>
  <strong>Cep:</strong>
  <%= @candidato.cep %>
</p>

<p>
  <strong>Municipio:</strong>
  <%= @candidato.municipio %>
</p>

<p>
  <strong>Datanascimento:</strong>
  <%= @candidato.dataNascimento %>
</p>

<p>
  <strong>Naturalidade:</strong>
  <%= @candidato.naturalidade %>
</p>

<p>
  <strong>Nacionalidade:</strong>
  <%= @candidato.nacionalidade %>
</p>

<p>
  <strong>Cpf:</strong>
  <%= @candidato.cpf %>
</p>

<p>
  <strong>Foto:</strong>
  <%= @candidato.foto %>
</p>

<fieldset>
  <legend>Certificados</legend>
  <% @candidato.certificados.each do |certificado|%>
    <p>
      <%= certificado.descricao %>
    </p>
  <%end%>
</fieldset>

<fieldset>
  <legend>Linguas</legend>
  <% @candidato.candidato_linguas.each do |lingua|%>
    <p>
      <%= lingua.lingua %>-<%= lingua.nivel %>
    </p>
  <%end%>
</fieldset>

<%= link_to 'Edit', edit_candidato_path(@candidato) %> |
<%= link_to 'Back', candidatos_path %>
