<p id="notice"><%= notice %></p>

<h1>Listing Graus Formacao</h1>

<table>
  <thead>
    <tr>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @graus_formacao.each do |grau_formacao| %>
      <tr>
        <td><%= grau_formacao.descricao %></td>
        <td><%= link_to 'Show', grau_formacao %></td>
        <td><%= link_to 'Edit', edit_grau_formacao_path(grau_formacao) %></td>
        <td><%= link_to 'Destroy', grau_formacao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Grau formacao', new_grau_formacao_path %>
